<launch>

	<arg name="rosbag_enabled" default="false"/>
	<arg name="rosbag_prefix_url" default="/tmp"/>

	<arg name="debug" default="False"/>
	<arg name="test" default="False"/>

	<arg name="route_service" default="$(arg route_service)"/>
	
	<node name="nautonomous_routing_vaarkaart" pkg="nautonomous_routing_vaarkaart" type="vaarkaart_routing_node.py"  output="screen" >
 		<param name="debug" value="$(arg debug)"/>
		<param name="test" value="$(arg test)"/>

		<remap from="route_service" to="$(arg route_service)"/>
	</node>

</launch>
